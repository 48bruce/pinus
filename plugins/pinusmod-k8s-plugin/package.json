{
  "name": "pinusmod-k8s-plugin",
  "version": "1.4.26",
  "description": "pinus base plugin",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && yarn format && yarn lint && tsc",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\" --fix",
    "start": "yarn run build && node dist/index",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus-monitor lib/"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "./dist/index",
  "types": "./src/index",
  "dependencies": {
    "pinusmod": "^1.4.26"
  },
  "devDependencies": {
    "@types/node": "^14",
    "typescript": "3.7.3"
  },
  "license": "MIT"
}
