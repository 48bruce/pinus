{
  "name": "pinusmod",
  "version": "1.4.26",
  "private": false,
  "homepage": "https://github.com/node-pinus/pinus",
  "repository": {
    "type": "git",
    "url": "git@github.com:node-pinus/pinus.git"
  },
  "scripts": {
    "build": "rimraf dist/* && tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "mochatest": "cd dist && mocha --require source-map-support/register --recursive",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/pinus lib/"
  },
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/node-pinus/pinus/issues"
  },
  "author": "mybios",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/node-pinus/pinus#license"
    }
  ],
  "keywords": [
    "pinus",
    "framework",
    "game",
    "web",
    "realtime",
    "server"
  ],
  "files": [
    "bin",
    "dist",
    "lib",
    "template",
    "test"
  ],
  "main": "./dist/lib/index",
  "types": "./lib/index",
  "dependencies": {
    "async": "^3.0.0",
    "cliff": "^0.1.10",
    "commander": "^8.0.0",
    "crc": "^4.1.0",
    "eyes": "^0.1.8",
    "mkdirp": "^1.0.4",
    "mqtt-connection": "^4.1.0",
    "node-bignumber": "^1.2.1",
    "pinusmod-admin": "^1.4.26",
    "pinusmod-loader": "^1.4.26",
    "pinusmod-logger": "^1.4.26",
    "pinusmod-protobuf": "^1.4.26",
    "pinusmod-protocol": "^1.4.26",
    "pinusmod-rpc": "^1.4.26",
    "pinusmod-scheduler": "^1.4.26",
    "seq-queue": "^0.0.5",
    "socket.io": "^4.1.3",
    "ws": "^8.3.0"
  },
  "bin": {
    "pinusmod": "./dist/bin/pinus.js"
  },
  "devDependencies": {
    "@types/async": "3.2.8",
    "@types/cliff": "0.1.6",
    "@types/commander": "2.12.2",
    "@types/crc": "3.4.0",
    "@types/eyes": "0.1.32",
    "@types/mkdirp": "1.0.2",
    "@types/mocha": "^5.0.0",
    "@types/mqtt-connection": "git+https://gitee.com/xxbruce/types-mqtt-connection.git",
    "mocha": "9.1.3",
    "@types/ws": "8.2.2",
    "muk": "0.5.3",
    "nyc": "15.1.0",
    "should": "13.2.3",
    "tslint": "6.1.3",
    "typescript": "^4.3.5"
  }
}
